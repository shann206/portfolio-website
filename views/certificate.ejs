<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/styles.css">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
  <title>Portfolio</title>
</head>

<body data-bs-spy="scroll" data-bs-target=".custom-navbar" data-bs-offset="70" tabindex="0">
  <%- include('partials/navbar') %>

<section class="category-header py-3">
  <div class="container">
    <nav aria-label="breadcrumb">
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/">Home</a></li>
        <li class="breadcrumb-item active" aria-current="page">Development</li>
      </ol>
    </nav>
    <h2 class="text-center">Hacker Rank Certificate</h2>
  </div>
</section>

<section id="category-projects" class="py-5">
  <div class="container">
    <div class="row g-4">
        <!-- Certificate 1-->
        <div class="col-12 col-sm-6 col-md-4 col-lg-3 d-flex">
          <div class="card w-100">
            <img class="card-img-top" src="/images/jsCert.png" alt="Javascript Certificate">
            <div class="card-body d-flex flex-column">
              <h5 class="card-title">Javascript Certificate</h5>
            </div>
          </div>
        </div>
      <!-- Certificate 2 -->
      <div class="col-md-6 col-lg-3">
          <div class="card h-100 portfolio-card">
            <img src="/images/reactCert.png" class="card-img-top" alt="React Certificate">
            <div class="card-body">
              <h5 class="card-title">React Certificate</h5>
            </div>
          </div>
      </div>
      
    </div>
  </div>
</section>

<div class="container mt-4">
    <div class="d-flex justify-content-between">
        <a href="/portfolio/uiux" class="btn btn-back">
            ← UI/UX projects
        </a>
        <a href="/portfolio" class="btn btn-back">
            Back to Portfolio →
        </a>
    </div>
</div>

<%- include('partials/footer') %>
<script src="/js/theme.js"></script>
</body>
</html>
